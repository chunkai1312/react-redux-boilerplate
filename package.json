{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "A React/Redux application boilerplate using material design",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist static/dist",
    "build-server": "babel src -d dist --copy-files",
    "build-client": "webpack --config build/webpack.prod.config.js --progress",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "start-prod": "cross-env NODE_ENV=production node src/server",
    "start-dev": "nodemon --watch src src/server",
    "watch-client": "node build/dev-server",
    "prod": "npm run build && npm run start-prod",
    "dev": "concurrently -k \"npm run watch-client\" \"npm run start-dev\"",
    "lint": "eslint src",
    "pretest": "webpack --config build/webpack.dev.config.js --progress",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.13.3",
    "express-session": "^1.14.0",
    "http-proxy-middleware": "^0.17.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.7",
    "react-toolbox": "1.3.4",
    "redux": "^3.2.1",
    "redux-actions": "^1.2.1",
    "redux-saga": "^0.14.3",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.1.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.8.5",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.13.0",
    "redux-mock-store": "^1.1.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-isomorphic-tools": "2.5.8"
  }
}

{
  "name": "react-redux-universal-starter",
  "version": "0.0.0",
  "description": "A starter kit for building universal React/Redux applications",
  "scripts": {
    "start": "node bin/server.js",
    "dev": "npm-run-all --parallel start:dev build:watch",
    "start:dev": "nodemon bin/server.js",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "rimraf dist static/dist",
    "build:server": "babel src -d dist --copy-files",
    "build:client": "webpack --config build/webpack.prod.config.js --progress",
    "build:watch": "node build/dev-server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkai1312/universal-react-redux-starter-kit.git"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "react",
    "reactjs",
    "redux",
    "boilerplate",
    "starter",
    "webpack"
  ],
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chunkai1312/universal-react-redux-starter-kit/issues"
  },
  "homepage": "https://github.com/chunkai1312/universal-react-redux-starter-kit",
  "dependencies": {
    "compression": "^1.7.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "http-proxy-middleware": "^0.17.4",
    "prop-types": "^15.5.0",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-mixins": "^6.0.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-isomorphic-tools": "^3.0.3"
  }
}
